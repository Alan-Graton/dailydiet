{
  "expo": {
    "name": "dailydiet",
    "slug": "dailydiet",
    "scheme": "dailydiet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/src/assets/Logo.png",
    "userInterfaceStyle": "auto",
    "splash": {
      "image": "./app/src/assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#CBE4B4"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/src/assets/Logo.png",
        "backgroundColor": "#CBE4B4"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#CBE4B4",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
